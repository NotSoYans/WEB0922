<body>

 <img src="grumpy.jpg" onmouseover='husky(this)' onmouseleave='husky_g(this)'/>

</body>

<script>
function husky(obj) {
	obj.src='happy.jpg';
	
}
function husky_g(obj) {
	obj.src='grumpy.jpg';
}
</script>

</html>
